{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./types-temp",
    "target": "esnext",
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "module": "esnext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": false,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "types": [
      "vite/client",
      "vitest/globals",
      "unplugin-vue-router/client",
      "vite-plugin-vue-layouts-next/client"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/util/*": [
        "src/util/*"
      ]
    },
    "skipLibCheck": true
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
