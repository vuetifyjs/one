{
  "name": "@vuetify/auth",
  "version": "0.1.7",
  "description": "Authentication for Vuetify services",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuetifyjs/one",
    "directory": "packages/auth"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./stores": {
      "types": "./dist/stores/index.d.mts",
      "default": "./dist/stores/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.mts",
      "default": "./dist/types/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "release": "pnpm build && bumpp --tag \"@vuetify/auth@%s\"",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "pinia": "^3.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "happy-dom": "catalog:",
    "pinia": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  }
}
