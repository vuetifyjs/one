{
  "name": "@vuetify/one",
  "version": "0.3.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.build.config.ts && yarn build:types",
    "build:dev": "vite build --mode development --config vite.build.config.ts ",
    "build:types": "rimraf types-temp && vue-tsc --pretty -p tsconfig.json && rollup --config rollup.types.config.js && rimraf types-temp",
    "watch": "yarn build:dev --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "@mdi/js": "6.2.95",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@mdi/js": "6.2.95",
    "@rollup/plugin-alias": "^5.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.15.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-vuetify": "^1.1.0",
    "conventional-github-releaser": "^3.1.5",
    "core-js": "^3.29.0",
    "eslint": "^8.46.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^23.0.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.0",
    "rimraf": "^5.0.5",
    "roboto-fontface": "*",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.0",
    "unplugin-fonts": "^1.0.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.2.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vitest": "^1.1.0",
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "vue-tsc": "^1.2.0",
    "vuetify": "^3.0.0"
  }
}
