{
  "name": "@vuetify/one",
  "version": "2.2.4",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/index.css"
    },
    "./subpath/*": "./dist/subpath/*.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.build.config.ts && pnpm build:types",
    "build:dev": "vite build --mode development --config vite.config.ts ",
    "build:types": "rimraf types-temp && vue-tsc --pretty -p tsconfig.json && rollup --config rollup.types.config.js && rimraf types-temp",
    "release": "pnpm build:dev && pnpm version && git push --follow-tags",
    "watch": "pnpm build:dev --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "@mdi/js": "7.4.47",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.13",
    "vuetify": "^3.8.0"
  },
  "devDependencies": {
    "@babel/types": "^7.27.1",
    "@fontsource/roboto": "^5.2.5",
    "@mdi/js": "7.4.47",
    "@rollup/plugin-alias": "^5.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vueuse/router": "^13.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-vuetify": "^2.0.2",
    "conventional-github-releaser": "^3.1.5",
    "core-js": "^3.42.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.27.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-vuetify": "^4.0.0",
    "jsdom": "^26.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "sass": "^1.89.0",
    "taze": "^19.1.0",
    "typescript": "5.8.3",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10",
    "vuetify": "^3.8.6"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "vite-plugin-pages": "^0.33.0",
    "vite-plugin-vue-layouts-next": "^0.1.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "unrs-resolver"
    ]
  }
}
