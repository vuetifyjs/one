{
  "name": "@vuetify/one",
  "version": "1.9.5",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/style.css"
    },
    "./subpath/*": "./dist/subpath/*.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:one": "vite build --config vite.build.config.ts && pnpm build:types",
    "build:dev": "vite build --mode development --config vite.build.config.ts ",
    "build:types": "rimraf types-temp && vue-tsc --pretty -p tsconfig.json && rollup --config rollup.types.config.js && rimraf types-temp",
    "release": "pnpm build:dev && pnpm version && git push --follow-tags",
    "watch": "pnpm build:dev --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build:one",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "@mdi/js": "7.4.47",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.0",
    "vuetify": "^3.6.1"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@mdi/js": "7.4.47",
    "@rollup/plugin-alias": "^5.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/router": "^10.11.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-vuetify": "^1.2.1",
    "conventional-github-releaser": "^3.1.5",
    "core-js": "^3.37.1",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-vuetify": "^1.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "rimraf": "^5.0.8",
    "roboto-fontface": "*",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "sass": "^1.77.6",
    "swetrix": "^3.5.2",
    "taze": "^0.14.2",
    "typescript": "5.3.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26",
    "vuetify": "^3.6.11"
  },
  "packageManager": "pnpm@9.10.0"
}
