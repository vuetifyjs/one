{
  "name": "@vuetify/one",
  "version": "2.11.1",
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuetifyjs/one"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./stores/auth": "./dist/stores/auth.js",
    "./styles": {
      "import": "./dist/one.css"
    },
    "./subpath/*": "./dist/subpath/*.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.build.config.ts && pnpm build:types",
    "build:dev": "vite build --mode development --config vite.config.ts ",
    "build:types": "rimraf types-temp && vue-tsc --pretty -p tsconfig.json && rollup --config rollup.types.config.js && rimraf types-temp",
    "release": "pnpm build:dev && bumpp -r",
    "watch": "pnpm build:dev --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue && vue-tsc --pretty -p tsconfig.json --noEmit",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@mdi/js": "7.4.47",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.13",
    "vuetify": "^3.8.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@fontsource/roboto": "^5.2.6",
    "@mdi/js": "7.4.47",
    "@rollup/plugin-alias": "^5.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.9",
    "@vueuse/router": "^13.9.0",
    "bumpp": "^10.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-vuetify": "^2.0.2",
    "conventional-github-releaser": "^3.1.5",
    "core-js": "^3.45.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.35.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-vuetify": "^4.1.0",
    "jsdom": "^26.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "sass": "^1.92.1",
    "taze": "^19.6.0",
    "typescript": "5.8.3",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.0.7",
    "vite-plugin-vuetify": "^2.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.6",
    "vuetify": "^3.10.0"
  },
  "packageManager": "pnpm@10.26.1",
  "dependencies": {
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-vue-layouts-next": "^1.0.0"
  }
}
